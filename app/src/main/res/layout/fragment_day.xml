<RelativeLayout
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="mordbad.master.DayFragment"
    >
    <!--layout_height="">-->

    <!-- TODO: Update blank fragment layout -->
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="Forslag 1:"
        android:onClick="onClick"
        android:clickable="true"
        android:id="@+id/tulleteksten"/>

    <Button
        android:id="@+id/startTheApocalypse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Discard day and get new."
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="46dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="50dp"
        android:text="Forslag 2:"
        android:onClick="onClick"
        android:clickable="true"/>

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView7"
        android:layout_below="@+id/textView7"
        android:layout_marginTop="43dp"
        android:text="Forslag 3:"
        android:onClick="onClick"
        android:clickable="true"/>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView8"
        android:layout_below="@+id/textView8"
        android:layout_marginTop="36dp"
        android:text="Forslag 4:"
        android:onClick="onClick"
        android:clickable="true"/>

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView9"
        android:layout_below="@+id/textView9"
        android:layout_marginTop="39dp"
        android:text="Forslag 5:"
        android:onClick="onClick"
        android:clickable="true"/>

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView10"
        android:layout_below="@+id/textView10"
        android:layout_alignParentStart="true"
        android:text="Link til sti:"
        android:onClick="onClick"
        android:clickable="true"/>

</RelativeLayout>
